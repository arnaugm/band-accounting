<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Band accounting{% endblock %}</title>
    {% block stylesheets %}
        <link href="{{ asset('vendor/angular-material/angular-material.min.css') }}" rel="stylesheet" />
        <link href="{{ asset('css/styles.css') }}" rel="stylesheet" />
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body ng-app="app" layout="column">
{% block body %}{% endblock %}
{% block javascripts %}
    <script src="{{ asset('vendor/angular/angular.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('vendor/angular-animate/angular-animate.min.js') }}" type="text/javascript" ></script>
    <script src="{{ asset('vendor/angular-aria/angular-aria.min.js') }}" type="text/javascript" ></script>
    <script src="{{ asset('vendor/angular-material/angular-material.min.js') }}" type="text/javascript" ></script>
    <script src="{{ asset('vendor/angular-resource/angular-resource.min.js') }}" type="text/javascript" ></script>
    <script src="{{ asset('js/app.js') }}" type="text/javascript" ></script>
    <script src="{{ asset('js/activity-model.js') }}" type="text/javascript" ></script>
    <script src="{{ asset('js/edit-activity-directive.js') }}" type="text/javascript" ></script>
    <script src="{{ asset('js/activities-list-directive.js') }}" type="text/javascript" ></script>
    <script src="{{ asset('js/activity-resource.js') }}" type="text/javascript" ></script>
    <script src="{{ asset('js/date-helper.js') }}" type="text/javascript" ></script>
{% endblock %}
</body>
</html>