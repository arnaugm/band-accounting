<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Band accounting{% endblock %}</title>
    {% block stylesheets %}
        <link href="{{ asset('css/angular-material.min.css') }}" rel="stylesheet" />
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,400italic">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link href="{{ asset('css/styles.css') }}" rel="stylesheet" />
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>

<body ng-app="app" layout="column">

{% block body %}
{% endblock %}

{% block javascripts %}
    <script src="{{ asset('vendor.js') }}" type="text/javascript"></script>
    <script src="{{ asset('scripts.js') }}" type="text/javascript"></script>
{% endblock %}

</body>
</html>
